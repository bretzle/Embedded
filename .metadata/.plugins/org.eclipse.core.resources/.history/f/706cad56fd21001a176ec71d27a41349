  * @author  Auto-generated by STM32CubeIDE
  * @version V1.0
  * @brief   Default main function.
  ******************************************************************************
*/


#include "lcd.h"
#include "timing.h"

// main
int main(void){
	lcd_init();
	lcd_clear();
	lcd_home();

	int time = 10;
	for (int i = 0; i < 10; i++) {
		lcd_print_num(time);
		delay_ms(500);
		lcd_clear();
		time--;
	}

	lcd_print_string("Finished!");

	while(1);
	return 0;
}

