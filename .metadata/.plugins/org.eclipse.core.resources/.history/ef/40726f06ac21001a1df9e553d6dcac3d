#include "timing.h"
#include <inttypes.h>
#include <stdio.h>
#include <string.h>


void lcd_init(void) {
	enable_GPIO('B');
	init_PB0_to_PB2_mode();

	enable_GPIO('C');
	init_PC8_to_PC11_mode();

	lcd_set_up();
}

void enable_GPIO(char port) {
	volatile uint32_t* rcc_ahb1enr = (uint32_t*) RCC_AHB1ENR;

	switch (port) {
	case 'A':
		break;
	case 'B':
		*rcc_ahb1enr |= GPIOBEN;
		break;
	case 'C':
		*rcc_ahb1enr |= GPIOCEN;
		break;
	case 'D':
		break;
	case 'E':
		break;
	case 'F':
		break;
	case 'G':
		break;
	case 'H':
		break;
	}
}

